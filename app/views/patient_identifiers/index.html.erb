<p id="notice"><%= notice %></p>

<h1>Patient Identifiers</h1>

<table>
  <thead>
    <tr>
      <th>Patient identifier</th>
      <th>Patient</th>
      <th>Identifier</th>
      <th>Identifier type</th>
      <th>Preferred</th>
      <th>Location</th>
      <th>Creator</th>
      <th>Date created</th>
      <th>Voided</th>
      <th>Voided by</th>
      <th>Date voided</th>
      <th>Void reason</th>
      <th>Uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_identifiers.each do |patient_identifier| %>
      <tr>
        <td><%= patient_identifier.patient_identifier_id %></td>
        <td><%= patient_identifier.patient_id %></td>
        <td><%= patient_identifier.identifier %></td>
        <td><%= patient_identifier.identifier_type %></td>
        <td><%= patient_identifier.preferred %></td>
        <td><%= patient_identifier.location_id %></td>
        <td><%= patient_identifier.creator %></td>
        <td><%= patient_identifier.date_created %></td>
        <td><%= patient_identifier.voided %></td>
        <td><%= patient_identifier.voided_by %></td>
        <td><%= patient_identifier.date_voided %></td>
        <td><%= patient_identifier.void_reason %></td>
        <td><%= patient_identifier.uuid %></td>
        <td><%= link_to 'Show', patient_identifier %></td>
        <td><%= link_to 'Edit', edit_patient_identifier_path(patient_identifier) %></td>
        <td><%= link_to 'Destroy', patient_identifier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Identifier', new_patient_identifier_path %>
