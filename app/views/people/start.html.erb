<p id="notice"><%= notice %></p>

<h1>Potential Duplicates</h1>

<table>
  <thead>
    <tr>
      <th>Patient identifier</th>
      <th>Patient</th>
      <th>Identifier</th>
      <th>Identifier type</th>
      <th>Preferred</th>
      <th>Location</th>
      <th>Creator</th>
      <th>Date created</th>
      <th>Voided</th>
      <th>Voided by</th>
      <th>Date voided</th>
      <th>Void reason</th>
      <th>Uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |patient| %>
      <tr>
        <td><%= patient['patient_id'] %></td>
        <td><%= patient['npid'] %></td>
        <td><%= patient['given_name'] %></td>
        <td><%= patient['family_name'] %></td>
        <td><%= patient['gender'] %></td>
        <td><%= patient['birthdate'] %></td>
        <td><%= patient['birthdate_estimated']%></td>
        <td><%= patient['address1'] %></td>
        <td><%= patient['address2'] %></td>
        <td><%= patient['city_village'] %></td>
        <td><%= patient['state_province'] %></td>
        <td><%= patient['county_district'] %></td>
        <td><%= patient['neighborhood_cell']%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>